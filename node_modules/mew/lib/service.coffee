Moment = require 'moment'

ignored_functions=["start","stop"]

class MewService
    constructor : (@mew)->

    start : (callback)->
        callback()

    ping : (callback)->
        response = {
            status : "NORMAL",
            name : @mew.name,
            time : (new Moment()).format()
        }
        callback(null,response)

    stop : ->



module.exports=MewService
module.exports.ignored_functions=ignored_functions
